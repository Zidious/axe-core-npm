{
  "name": "@axe-core/cli",
  "version": "4.6.0",
  "description": "A CLI for accessibility testing using axe-core",
  "author": {
    "name": "Wilco Fiers",
    "organization": "Deque Systems, Inc.",
    "url": "http://github.com/wilcofiers/"
  },
  "contributors": [
    {
      "name": "Michael Siek (me@michaelsiek.com)"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dequelabs/axe-core-npm.git"
  },
  "license": "MPL-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "bin": {
    "axe": "./dist/cli.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:lib && npm run build:bin && npm run build:types",
    "build:bin": "tsup src/bin/cli.ts",
    "build:lib": "tsup src/lib/index.ts --dts --format esm,cjs",
    "build:types": "tsup src/types.ts --dts-only",
    "test": "mocha --timeout 60000",
    "coverage": "c8 npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "axe-core",
    "accessibility",
    "a11y",
    "wcag",
    "cli",
    "testing"
  ],
  "dependencies": {
    "@axe-core/webdriverjs": "^4.6.0",
    "axe-core": "^4.6.1",
    "chromedriver": "^109.0.0",
    "colors": "^1.4.0",
    "commander": "^9.4.1",
    "selenium-webdriver": "^4.8.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chromedriver": "^81.0.1",
    "@types/mocha": "^10.0.0",
    "@types/mock-fs": "^4.13.1",
    "@types/selenium-webdriver": "^4.1.5",
    "c8": "^7.13.0",
    "chai": "^4.3.6",
    "execa": "5.1.0",
    "mocha": "^10.0.0",
    "mock-fs": "^5.1.4",
    "rimraf": "^3.0.2",
    "tempy": "1.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.8.4"
  },
  "c8": {
    "checkCoverage": true,
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 95,
    "branches": 92,
    "functions": 100,
    "lines": 95,
    "exclude": [
      "dist",
      "coverage",
      "**/*.test.ts",
      "src/testutils"
    ]
  }
}
